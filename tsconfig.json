{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": { "*": ["node_modules"] },
		"target": "es6",
		"module": "commonjs",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"noUnusedParameters": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"allowSyntheticDefaultImports": true
	}
}
